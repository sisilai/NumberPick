<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="max" format="integer" />
    <attr name="min" format="integer" />
    <attr name="number" format="integer" />

    <declare-styleable name="NumberPick">
        <attr name="max" />
        <attr name="min" />
        <attr name="number" />
    </declare-styleable>

    <declare-styleable name="Np">
        <attr name="max" />
        <attr name="min" />
        <attr name="number" />
        <attr name="lineColor" format="color" />
        <attr name="numberColor" format="color" />
        <attr name="corner" format="dimension" />
        <attr name="width" format="dimension" />
    </declare-styleable>

</resources>